{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "private": true,
  "description": "A simple Discord bot template, featured with an awesome new thing on Discord.",
  "repository": "https://github.com/zhycorp/discord-bot-template",
  "license": "AGPL-3.0",
  "author": "Zhycorp",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r dotenv/config src/bot.ts"
  },
  "main": "dist/index.js",
  "eslintConfig": {
    "extends": "@zhycorp/eslint-config/typescript",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "dependencies": {
    "date-fns": "^2.23.0",
    "discord.js": "github:discordjs/discord.js",
    "got": "^11.8.2",
    "tslib": "^2.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@zhycorp/eslint-config": "^3.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "erlpack": "github:discord/erlpack",
    "libsodium-wrappers": "^0.7.9",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
